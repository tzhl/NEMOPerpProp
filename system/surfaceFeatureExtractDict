FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      surfaceFeatureExtractDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
NEMO4_Boom.stl
{
    extractionMethod    extractFromSurface;
    extractFromSurfaceCoeffs{includedAngle   150;}
    subsetFeatures{openEdges       yes;}
    trimFeatures
    {
        minElem          0;
        minLen           0;
    }
    writeObj                yes;  
}
